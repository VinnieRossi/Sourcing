{
    "routes": [
        {
            "route": "/*",
            "serve": "/index.html",
            "statusCode": 200
        }
    ],
    "defaultHeaders": {
        "content-security-policy": "default-src https: 'unsafe-eval' 'unsafe-inline' wss:; object-src 'none'"
    },
    "mimeTypes": {
        "custom": "text/html"
    }
}